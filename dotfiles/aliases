alias df="df -h"
alias du="du -h"
alias gh="git log --graph --pretty='format:%Cred%h%Cblue %ad %Creset%s %Cgreen%d%Creset' --date=format:%y.%m.%d"
alias gha="gh --all"
alias gp="git push --tags&&git push"
alias poweroff="sudo poweroff -f"
alias reboot="sudo reboot -f"
alias l='ls --group-directories-first --color -hlF'
alias ll='l -a -v'
alias vb='vboxmanage'
alias vbls='vb list vms'
alias vbps='vb list runningvms'
alias dps='docker ps --format "table {{.Names}}	{{.Status}}	{{.Ports}}"'
alias dc="docker-compose"
