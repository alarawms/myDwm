alias df="df -h"
alias du="du -h"
alias gh="git log --graph --pretty='format:%Cblue%h%Creset %ad %Cred%<(8,trunc)%an%Creset %s %Cgreen%d%Creset' --date=short"
alias gha="gh --all"
alias gp="git push --tags&&git push"
alias poweroff="sudo poweroff -f"
alias reboot="sudo reboot -f"
alias ls='ls --group-directories-first --color -hF'
alias l='ls -lh'
alias ll='ls -lha'
alias vb='vboxmanage'
alias vbwin7='vb startvm win7'
alias vbwin10='vb startvm win10'
alias vbls='vb list vms'
alias vbps='vb list runningvms'
alias dps='docker ps --format "table {{.Names}}	{{.Status}}	{{.Ports}}"'
alias dc="docker-compose"
